#include <iostream>
using namespace std;
int main()
{
	const int n = 5;
	int i, c = 0;
	double Res = 0;
	int a[n];
	for (i = 0; i < n; i++)
	{
		cin >> a[i];
		if (a[i] % 2 == 1)
		{
			Res += a[i];
			c++;
		}
	}
	if (c == 0) 
		cout << "nooooooo";
	else
	{
		Res = Res / c;
		cout << "Res = " << Res;
	}

	return 0;
}
